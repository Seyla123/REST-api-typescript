{
    "version": 2,
    "builds": [
      {
        "src": "src/server.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["dist/**/*", "node_modules/**/*"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/server.js"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }
  