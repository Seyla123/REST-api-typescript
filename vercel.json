{
    "version": 2,
    "builds": [
      {
        "src": "dist/server.js",
        "use": "@vercel/node"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "hooks": {
      "build": "npm run build"
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/server.js"
      }
    ]
  }
  